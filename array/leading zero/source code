#include <stdio.h>
#include<string.h>
int main()
{
    int i,b,l;
    char s[100]="0000345";
    l=strlen(s);
    for(i=0;i<l;i++)
    {
        if(s[i]!=0)
        {
            b=i;
            break;
        }
    }
for(i=b;i<l;i++)
{
    printf("%c",s[i]);
}
    return 0;
}
